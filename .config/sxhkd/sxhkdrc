XF86MonBrightnessDown
  set_brightness down && kill -36 $(pidof dwmblocks)

XF86MonBrightnessUp
  set_brightness up && kill -36 $(pidof dwmblocks)

XF86AudioPrev
  mpc prev

XF86AudioPlay
  mpc toggle

XF86AudioNext
  mpc next

XF86AudioRaiseVolume
  pamixer -i 5 && kill -35 $(pidof dwmblocks)

XF86AudioLowerVolume
  pamixer -d 5 && kill -35 $(pidof dwmblocks)

XF86AudioMute
  pamixer -t && kill -35 $(pidof dwmblocks)

